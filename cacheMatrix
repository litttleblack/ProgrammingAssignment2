makeCacheMatrix <- function(x=matrix()){
	v <- NULL
	set <- function(y){
		x <<- y
	}
	get <- function() x
	
	setInverse <- function(inverse) v <<- inverse
	getInverse <- function() v
	
	list(set=set, get=get, setInverse=setInverse, getInverse=getInverse)
}

cacheSolve <- function(x){
	v <- x$getInverse()
	if(!is.null(v)){
		message("Here you are!")
		return(v)
	}
	data <- x$get()
	v <- solve(data)
	x$setInverse(v)
	v
}
